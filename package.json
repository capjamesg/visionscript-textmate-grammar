{
  "name": "visionscript",
  "displayName": "VisionScript",
  "description": "A VisionScript programming language syntax highlighter.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/capjamesg/visionscript-textmate-grammar"
  },
  "icon": "assets/logo.png",
  "license": "MIT",
  "publisher": "visionscript",
  "contributes": {
    "themes": [
      {
        "label": "VisionScript",
        "uiTheme": "vs-dark",
        "path": "./themes/vic.tmTheme.json"
      }
    ],
    "languages": [
      {
        "id": "vic",
        "aliases": [
          "VisionScript",
          "vic"
        ],
        "extensions": [
          ".vic"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "hoverProvider": true,
    "grammars": [
      {
        "language": "vic",
        "scopeName": "source.visionscript",
        "path": "./syntaxes/vic.tmLanguage.json"
      }
    ]
  },
  "configurationDefaults": {
    "visionscript": {
      "editor.semanticHighlighting.enabled": true
    }
  },
  "dependencies": {
    "front-matter": "^4.0.2"
  },
  "pricing": "Free"
}
